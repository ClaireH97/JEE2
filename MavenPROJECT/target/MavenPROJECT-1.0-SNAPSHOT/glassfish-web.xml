<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE glassfish-web-app PUBLIC "-//GlassFish.org//DTD GlassFish Application Server 3.1 Servlet 3.0//EN" "http://glassfish.org/dtds/glassfish-web-app_3_0-1.dtd">
<glassfish-web-app error-url="">
  <context-root>/MavenPROJECT/</context-root>
  <class-loader delegate="true"/>

     <servlet>
        <servlet-name>Controller</servlet-name>
        <servlet-class>com.mycompany.mavenproject.Controller</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Controller</servlet-name>
        <url-pattern>/com.mycompany.mavenproject/Controller</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>Controller</welcome-file>
    </welcome-file-list>
    <jsp-config>
        <jsp-property-group>
            <url-pattern>*.jsp</url-pattern>
            <include-prelude>jstl_config.jsp</include-prelude>
        </jsp-property-group>
    </jsp-config>
</glassfish-web-app>
